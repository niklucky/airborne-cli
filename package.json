{
  "name": "airborne-cli",
  "version": "0.0.1",
  "description": "Airborne CLI framework",
  "main": "dist/index.js",
  "scripts": {
    "precommit-test": "npm run cover && npm run check-coverage",
    "precommit": "npm run build",
    "check-coverage": "nyc check-coverage --statements 60 --branches 60 --functions 30 --lines 60",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha test --recursive --compilers js:babel-register",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t",
    "lint": "eslint src",
    "build:watch": "eslint src && babel -w src -d dist",
    "prebuild": "rimraf dist",
    "build": "eslint src && babel src -d dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niklucky/airborne-cli.git"
  },
  "keywords": [
    "ariborne",
    "engine",
    "framework",
    "api",
    "dao",
    "mapper",
    "controller",
    "service",
    "model",
    "cli",
    "mysql",
    "redis",
    "database"
  ],
  "files": [
    "dist",
    "docs",
    "examples",
    "README.md"
  ],
  "author": "Nikita Zhuravlev <iam@niklucky.com>",
  "license": "MIT",
  "homepage": "https://github.com/niklucky/airborne-cli#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2"
  },
  "dependencies": {
    "commander": "^2.9.0"
  },
  "engines": {
    "node": ">=4.1.2 =<6.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
